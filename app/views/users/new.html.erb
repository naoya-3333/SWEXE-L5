<%= form_for @user, url:{action: :create} do |f| %>
    <%= 'ユーザID：' %>
        <%= f.text_field :uid %><br>
    <%= f.label :password, 'パスワード' %>
        <%= f.password_field :password %>
    <%= f.label :password_confirmation, 'パスワード(確認)' %>
        <%= f.password_field :password_confirmation %>
    <%= '年齢：' %>
        <%= f.number_field :age %><br>
    <%= f.submit '登録' %>
    
<% end %>